<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">
    <!--TextBlock默认字体-->
    <FontFamily x:Key="TextBlock_FontFamily_Default">Microsoft YaHei</FontFamily>
    <!--TextBlock默认字体大小-->
    <sys:Double x:Key="TextBlock_FontSize_Default">12</sys:Double>
    <!--TextBlock默认字体样式-->
    <FontStyle x:Key="TextBlock_FontStyle_Default">Normal</FontStyle>
    <!--TextBlock默认字体粗细-->
    <FontWeight x:Key="TextBlock_FontWeight_Default">Normal</FontWeight>
    <!--TextBlock默认前景色-->
    <SolidColorBrush x:Key="TextBlock_Foreground_DefaultColor" Color="#111111" />

    <!--边框默认宽度-->
    <Thickness x:Key="Border_Thickness_DefaultValue" Bottom="1" Left="1" Right="1" Top="1" />
    <!--内边距默认值-->
    <Thickness x:Key="Padding_DefaultValue" Bottom="1" Left="1" Right="1" Top="1" />
    <!--外边距默认值-->
    <Thickness x:Key="Margin_DefaultValue" Bottom="1" Left="1" Right="1" Top="1" />

    <!--页面背景色-->
    <SolidColorBrush x:Key="Page_Background_DefaultColor" Color="#666666"></SolidColorBrush>
    <!--页面边框颜色-->
    <SolidColorBrush x:Key="Page_Borderground_DefaultColor" Color="#666666"></SolidColorBrush>
</ResourceDictionary>