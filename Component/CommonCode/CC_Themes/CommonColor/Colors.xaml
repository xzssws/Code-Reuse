<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">
    <!--  窗体背景色 背景色  -->
    <Color x:Key="WindowBackgroundColor">#040827</Color>

    <!--  普通 背景色  -->
    <Color x:Key="BackgroundColor">#FF454967</Color>
    <!--  深色 背景色  -->
    <Color x:Key="DarkBackgroundColor">#404E4E4E</Color>
    <!--  浅色 背景色  -->
    <Color x:Key="LightBackgroundColor">#333650</Color>

    <!--  默认前景色  -->
    <Color x:Key="ForegroundColor">#FFFFFF</Color>
    <!--  深色 前景色  -->
    <Color x:Key="LightForegroundColor">#FFA0CBEF</Color>
    <!--  浅色 前景色  -->
    <Color x:Key="DarkForegroundColor">#FF476D8D</Color>

    <!--  边框色  -->
    <Color x:Key="BorderColor">#80FFFFFF</Color>

    <!--  禁用 背景色  -->
    <Color x:Key="DisabledBackgroundColor">#15FFFFFF</Color>
    <!--  浅色 禁用背景色  -->
    <Color x:Key="LightDisabledBackgroundColor">#30FFFFFF</Color>

    <!--  禁用 背景色  -->
    <Color x:Key="DisabledForegroundColor">#939393</Color>
    <!--  浅色 禁用背景色  -->
    <Color x:Key="LightDisabledForegroundColor">#FF939393</Color>

    <!--  高亮颜色  -->
    <Color x:Key="HighlightColor">#566378</Color>
    <!--  烫色  -->
    <Color x:Key="HotColor">#60FFFFFF</Color>

</ResourceDictionary>