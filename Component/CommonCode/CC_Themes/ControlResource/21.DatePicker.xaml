                                                    Grid.Row="1"
                                                    Grid.RowSpan="3"
                                                    Grid.ColumnSpan="4"
                                                    Margin="0,-1,0,0"
                                                    BorderBrush="#BF86A7AC"
                                                    BorderThickness="1"
                                                    CornerRadius=".5"
                                                    Opacity="1" />
                                            <Path Grid.Row="1"
                                                  Grid.RowSpan="3"
                                                  Grid.Column="0"
                                                  Grid.ColumnSpan="4"
